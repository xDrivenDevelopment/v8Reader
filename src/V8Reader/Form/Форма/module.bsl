	ФайлКонфигурации = КаталогВременныхФайлов() + "КонфигурацияПоставщика.cf";
	ВыводРезультатаДляCfEpf(ФайлКонфигурации, КаталогДляРаспаковкиКонфигурации);
	Фильтр = 1;
	ВыводРезультатаСравнения();
